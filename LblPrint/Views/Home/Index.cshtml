@{
    ViewData["Title"] = "Home Page";
}

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <p>Learn about <a href="https://learn.microsoft.com/aspnet/core">building Web apps with ASP.NET Core</a>.</p>
</div>

<body>  
    <div class="label-container">
        <div class="label-outline-left">
            <label>Enter Part Number:</label> <br />
            <input type="text" id="PartNum" /><br />

            <label>Enter or Edit Location:</label> <br />
            <input type="text" id="Loc" /><br />

            <label>Enter FIFO Date(MM/YYYY):</label> <br />
            <input type="text" id="FIFO" /><br />

            <label>Enter Quantity of Labels:</label> <br />
            <input type="text" id="QTYLbl" /><br />

            <br />
            <input type="submit" id="PrintLabel" value="Print Label(s)" />
        </div>

        <div class="label-outline-right">
            <label id="lblPartNum"> Part Number </label> <br />
            <label id="lblDesc"> Description </label> <br />
            <label id="lblLocation"> Warehouse Location </label> <br /> 
            <label id="lblFIFO"> FIFO Date </label> <br />
        </div>
    </div>
</body>