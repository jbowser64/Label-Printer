@{
    
}
@model PartModel
<div class="text-center">
    <h1 class="display-4">Welcome!</h1>

</div>
<body>
    <div class="label-container">
        <div class="label-outline-left">
            <div class="label-entry">
                <label>Part Number:</label> <br />
                <input type="text" id="PartNum" name="partNum" placeholder="Enter Part Number" autofocus /><br />
                <br />

                <label>Location</label> <br />
                <input type="text" placeholder="Enter or Edit Location" id="Loc" /><br />
                <br />
                <label>FIFO:</label> <br />
                <input type="text" placeholder="Enter FIFO Date" id="FIFO" /><br />
                <br />

                <label>Labels:</label> <br />
                <input type="text" placeholder="Enter Label QTY" id="QTYLbl" /><br />

                <br />
                <input type="submit" id="PrintLabel" value="Print Label(s)" />
            </div>
        </div>

        <div class="label-outline-right">
            <div class="label">

              
                    <label id="lblPartNum"> @Model.Material </label> <br />
                    <label id="lblDesc"> @Model.Material_description </label>  <br />
                    <label id="lblLocation"> @Model.Bin </label> <br />
                    <label id="lblFIFO">  </label> <br />
               
            </div>
        </div>
    </div>
</body>